<div class="uk-section uk-section-small">
    <div class="uk-container">

        <div class="uk-grid">

            <div class="uk-width-1-1">

                <table class="uk-table uk-table-divider uk-table-responsive">
                    <tbody>
                        @{int count = 0; }
                        @foreach (var step in Model.tb_Setting.Steps.Split("*"))
                        {
                            count++;
                            <tr>
                                <td class="uk-width-1-3@m deeper">
                                    <div class="uk-grid uk-grid-small uk-flex uk-flex-middle">
                                        <div class="uk-width-auto">
                                            <div class="in-icon-wrapper theme-color">
                                                <i class="fas fa-user fa-lg"></i>
                                            </div>
                                        </div>
                                        <div class="uk-width-expand">
                                            <h3>STEP @count</h3>
                                        </div>
                                    </div>
                                </td>
                                <td class="deeper">@step</td>
                                <td class="uk-width-1-5@m uk-text-left uk-text-right@m">
                                    <a href="@Model.tb_Setting.StepLinks.Split(",")[count-1]" target="_blank" class="uk-button uk-button-text">Watch Video Guide<i class="fas fa-play fa-xs uk-margin-small-left"></i></a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>
